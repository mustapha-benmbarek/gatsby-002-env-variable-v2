  "dependencies": {
    "gatsby": "^2.17.6",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
