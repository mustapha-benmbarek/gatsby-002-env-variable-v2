{
  "name": "gatsby-002-env-variable-v2",
  "private": true,
  "description": "A simple helloWorld started with environment variables",
  "version": "1.0.0",
  "license": "Mustapha Benmbarek",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
